{
  "name": "vad-js",
  "version": "0.0.0",
  "description": "Accurate and performant VAD package for the browser.",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./vadNodeWorklet": "./dist/vadNodeWorklet.js"
  },
  "types": "./dist/index.d.ts",
  "scripts": {
    "build-fast": "tsup --config tsup.config.ts",
    "build": "pnpm run build-fast -- --dts --dts-resolve",
    "test": "vitest run",
    "prepublishOnly": "pnpm run build"
  },
  "license": "MIT",
  "devDependencies": {
    "@egoist/prettier-config": "1.0.0",
    "@types/audioworklet": "^0.0.40",
    "@types/emscripten": "^1.39.6",
    "prettier": "2.8.4",
    "tsup": "6.6.3",
    "typescript": "4.9.5",
    "vitest": "0.28.5"
  }
}
